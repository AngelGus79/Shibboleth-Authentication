# Configuration for NSS shib module
url = "https://<%= sp_hostname %>/nss.php";

# Used for SSL/TLS verification
#cafile = "file_ca.pem";
sslcheck = "false";

# Credentials for basic authentication (if any)
username = "user";
password = "password";

# Eventual cookie values to be passed to the HTTP call
# values with ${name} will be considered variables to be searched in the environment
cookie_num = "1";
cookie_1_name = "_shibsession_${Shib_Session_Unique}";
cookie_1_value = "${Shib_Session_ID}";
